/* The DDD Cargo sample application modeled in CML. Note that we split the application into multiple bounded contexts. */
ContextMap DDDMapPropiedadesAlpesAsIs {
	contains CapturaDeDatosManualContext
	contains IngestionDeDatosFuenteContext
	contains ProcesamientoDeDatosDeRegistroContext
	contains ProcesamientoDeDatosContractualesContext
	contains ProcesamientoDeDatosGeograficosContext
	contains ProcesamientoDeDatosCatastralesContext
	contains ValidacionDeDatosDeRegistroContext
	contains ValidacionDeDatosContractualesContext
	contains ValidacionDeDatosGeograficosContext
	contains ValidacionDeDatosCatastralesContext
	contains DisponibilizacionDeDatosContext
	contains AnalisisDeDatosContext

	CapturaDeDatosManualContext [U]->[D] ProcesamientoDeDatosDeRegistroContext

	IngestionDeDatosFuenteContext [U]->[D] ProcesamientoDeDatosDeRegistroContext

	CapturaDeDatosManualContext [U]->[D] ProcesamientoDeDatosContractualesContext

	IngestionDeDatosFuenteContext [U]->[D] ProcesamientoDeDatosContractualesContext

	CapturaDeDatosManualContext [U]->[D] ProcesamientoDeDatosGeograficosContext

	IngestionDeDatosFuenteContext [U]->[D] ProcesamientoDeDatosGeograficosContext

	CapturaDeDatosManualContext [U]->[D] ProcesamientoDeDatosCatastralesContext

	IngestionDeDatosFuenteContext [U]->[D] ProcesamientoDeDatosCatastralesContext

	ProcesamientoDeDatosDeRegistroContext [U]->[D] ValidacionDeDatosDeRegistroContext

	ProcesamientoDeDatosContractualesContext [U]->[D] ValidacionDeDatosContractualesContext

	ProcesamientoDeDatosGeograficosContext [U]->[D] ValidacionDeDatosGeograficosContext

	ProcesamientoDeDatosCatastralesContext [U]->[D] ValidacionDeDatosCatastralesContext

	ValidacionDeDatosDeRegistroContext [U]->[D] DisponibilizacionDeDatosContext

	ValidacionDeDatosDeRegistroContext [U]->[D] AnalisisDeDatosContext

	ValidacionDeDatosContractualesContext [U]->[D] DisponibilizacionDeDatosContext

	ValidacionDeDatosContractualesContext [U]->[D] AnalisisDeDatosContext

	ValidacionDeDatosGeograficosContext [U]->[D] DisponibilizacionDeDatosContext

	ValidacionDeDatosGeograficosContext [U]->[D] AnalisisDeDatosContext

	ValidacionDeDatosCatastralesContext [U]->[D] DisponibilizacionDeDatosContext

	ValidacionDeDatosCatastralesContext [U]->[D] AnalisisDeDatosContext

}

