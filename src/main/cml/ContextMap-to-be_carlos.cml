/* The DDD Cargo sample application modeled in CML. Note that we split the application into multiple bounded contexts. */
ContextMap DDDMapPropiedadesAlpesToBe {
	contains CapturaDeDatosManualContext
	contains IngestionDeDatosFuenteContext

	contains ProcesamientoDeDatosMinoristasContext
   	contains ProcesamientoDeDatosOficinasContext
	contains ProcesamientoDeDatosIndustrialContext
	contains ProcesamientoDeDatosEspecializadoContext

	contains ValidacionDeDatosMinoristasContext
   	contains ValidacionDeDatosOficinasContext
	contains ValidacionDeDatosIndustrialContext
	contains ValidacionDeDatosEspecializadoContext

    contains DisponibilizacionDeDatosContext
	contains AnalisisDeDatosContext

	CapturaDeDatosManualContext [U, OHS]->[D, ACL] ProcesamientoDeDatosMinoristasContext{}
	IngestionDeDatosFuenteContext [U, OHS]->[D, ACL] ProcesamientoDeDatosMinoristasContext
	ProcesamientoDeDatosMinoristasContext [U]->[D] ValidacionDeDatosMinoristasContext

    CapturaDeDatosManualContext [U, OHS]->[D, ACL] ProcesamientoDeDatosOficinasContext
	IngestionDeDatosFuenteContext [U, OHS]->[D, ACL] ProcesamientoDeDatosOficinasContext
	ProcesamientoDeDatosOficinasContext [U]->[D] ValidacionDeDatosOficinasContext

    CapturaDeDatosManualContext [U, OHS]->[D, ACL] ProcesamientoDeDatosIndustrialContext
	IngestionDeDatosFuenteContext [U, OHS]->[D, ACL] ProcesamientoDeDatosIndustrialContext
	ProcesamientoDeDatosIndustrialContext [U]->[D] ValidacionDeDatosIndustrialContext

    CapturaDeDatosManualContext [U, OHS]->[D, ACL] ProcesamientoDeDatosEspecializadoContext
	IngestionDeDatosFuenteContext [U, OHS]->[D, ACL] ProcesamientoDeDatosEspecializadoContext
	ProcesamientoDeDatosEspecializadoContext [U]->[D] ValidacionDeDatosEspecializadoContext

	/*ValidacionDeDatosMinoristasContext [U]->[D] DisponibilizacionDeDatosContext
   	ValidacionDeDatosOficinasContext [U]->[D] DisponibilizacionDeDatosContext
	ValidacionDeDatosIndustrialContext [U]->[D] DisponibilizacionDeDatosContext
	ValidacionDeDatosEspecializadoContext [U]->[D] DisponibilizacionDeDatosContext*/

	ValidacionDeDatosMinoristasContext [U]->[D] AnalisisDeDatosContext
   	ValidacionDeDatosOficinasContext [U]->[D] AnalisisDeDatosContext
	ValidacionDeDatosIndustrialContext [U]->[D] AnalisisDeDatosContext
	ValidacionDeDatosEspecializadoContext [U]->[D] AnalisisDeDatosContext
    
    AnalisisDeDatosContext [U]->[D] DisponibilizacionDeDatosContext

}




/* The original booking application context */
BoundedContext CapturaDeDatosManualContext {

}

BoundedContext IngestionDeDatosFuenteContext {

}


BoundedContext ProcesamientoDeDatosMinoristasContext {}
BoundedContext ProcesamientoDeDatosOficinasContext{}
BoundedContext ProcesamientoDeDatosIndustrialContext{}
BoundedContext ProcesamientoDeDatosEspecializadoContext{}

BoundedContext ValidacionDeDatosMinoristasContext {}
BoundedContext ValidacionDeDatosOficinasContext {}
BoundedContext ValidacionDeDatosIndustrialContext {}
BoundedContext ValidacionDeDatosEspecializadoContext {}


BoundedContext DisponibilizacionDeDatosContext {

}

BoundedContext AnalisisDeDatosContext {

}
